{
  "name": "@fabstir/llm-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Fabstir P2P LLM Marketplace",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
