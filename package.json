    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.3.5",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "ethers": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
