{
  "name": "@fabstir/llm-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Fabstir P2P LLM Marketplace",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "build:all": "yarn workspaces foreach run build && yarn build",
    "test:all": "yarn workspaces foreach run test && yarn test",
    "clean:all": "yarn workspaces foreach run clean && yarn clean"
  },
  "keywords": [
    "fabstir",
    "llm",
    "ai",
    "marketplace",
    "p2p",
    "blockchain",
    "base"
  ],
  "author": "Fabstir",
  "license": "MIT",
  "dependencies": {
    "@base-org/account": "^2.3.1",
    "@chainsafe/libp2p-noise": "^16.0.0",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@fabstir/llm-auth": "file:../fabstir-llm-auth",
    "@libp2p/bootstrap": "^11.0.13",
    "@libp2p/identify": "^3.0.0",
    "@libp2p/kad-dht": "^14.1.5",
    "@libp2p/mdns": "^11.0.14",
    "@libp2p/tcp": "^10.1.8",
    "@libp2p/websockets": "^9.0.11",
    "ethers": "^5.0.0",
    "events": "^3.3.0",
    "fake-indexeddb": "^6.2.2",
    "libp2p": "^2.4.1",
    "multiformats": "^13.3.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "dotenv": "^17.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.3.5",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "ethers": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
