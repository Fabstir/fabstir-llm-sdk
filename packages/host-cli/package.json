{
  "name": "@fabstir/host-cli",
  "version": "1.0.0",
  "description": "CLI tool for Fabstir host node management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "fabstir-host": "./bin/fabstir-host"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "fabstir",
    "llm",
    "blockchain",
    "cli",
    "host"
  ],
  "author": "Fabstir",
  "license": "MIT",
  "dependencies": {
    "@fabstir/sdk-core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.0.0",
    "inquirer": "^9.3.8",
    "keytar": "^7.9.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.19.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  }
}